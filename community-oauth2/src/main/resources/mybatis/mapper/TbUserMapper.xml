<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chenyu.community.oauth2.mapper.TbUserMapper">
    <select id="getUserByUsername" parameterType="string" resultType="tbUser">
        select * from tb_user where username = #{username}
    </select>

</mapper>